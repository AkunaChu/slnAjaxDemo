@{
    ViewData["Title"] = "HW1";
}

<h1>HW1</h1>

<div id="HW1" class="row row-cols-1 row-cols-md-3 g-4">
    
</div>

<script src="~/datas/travel.js"></script>
@section Scripts
    {

    <script>
        
        
        let datas = spots.CommonFormat.Section.map(
            employee => {
                let { CAT1, CAT2, stitle, xbody, idpt, xpostDate, file } = employee;
                var imgSrc = file && file.img && file.img[0] && file.img[0].text;
            return (

                `
          
          <div class="col">
          <div class="card">
          <img src="${imgSrc}" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title">${CAT1}: ${CAT2}</h5>
            <p class="card-text">${xbody}</p>
          </div>
          </div>
                  </div>
                `
            )

        })
                document.querySelector('#HW1').innerHTML = datas.join("");
    </script>

}
